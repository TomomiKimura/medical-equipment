{
  "name": "medical-equipment",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "npm run clean:kill-servers && turbo run dev --parallel",
    "fix": "run-s fix:*",
    "fix:lint": "turbo run verify:lint",
    "fix:style": "prettier --ignore-unknown --write .",
    "clean:kill-servers": "./scripts/kill-servers.sh",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "verify": "run-s verify:*",
    "verify:lint": "turbo run --parallel verify:lint",
    "verify:style": "prettier --cache '**/*.{js,jsx,ts,tsx,json,md,yaml,yml}' --check",
    "verify:types": "turbo run --parallel verify:types"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {
    "bootstrap-icons": "^1.10.2"
  }
}
